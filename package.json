{
    "name": "winston-cloudwatch-logs",
    "version": "0.0.1",
    "description": "A rewrite of the popular winston-cloudwatch package. Designed to be smaller, more effieicent, more modern and fully API compatible with the original package.",
    "repository": "https://github.com/alex-zissis/winston-cloudwatch-logs",
    "author": "Alex Zissis <alex@zico.dev>",
    "license": "MIT",
    "private": false,
    "main": "dist/cjs/index.js",
    "module": "dist/mjs/index.js",
    "exports": {
        ".": {
            "import": "./dist/mjs/index.js",
            "require": "./dist/cjs/index.js"
        }
    },
    "peerDependencies": {
        "winston": "^3.4.0"
    },
    "dependencies": {
        "@aws-sdk/client-cloudwatch-logs": "^3.47.0",
        "@aws-sdk/node-http-handler": "^3.47.0",
        "async": "^3.2.3",
        "chalk": "^5.0.0",
        "fast-safe-stringify": "^2.1.1",
        "lodash-es": "^4.17.21",
        "proxy-agent": "^5.0.0",
        "winston-transport": "^4.4.2"
    },
    "devDependencies": {
        "@types/async": "^3.2.12",
        "@types/chalk": "^2.2.0",
        "@types/lodash-es": "^4.17.5",
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.10",
        "@types/sinon": "^10.0.8",
        "@types/winston": "^2.4.4",
        "esmock": "^1.7.2",
        "mocha": "^9.1.4",
        "should": "^13.2.3",
        "sinon": "^12.0.1",
        "typescript": "^4.5.4"
    },
    "scripts": {
        "build": "rm -rf dist/* && npx tsc -p tsconfig.json && npx tsc -p tsconfig.cjs.json",
        "postbuild": "./postbuild",
        "postinstall": "yarn build",
        "test": "mocha --loader=esmock test/*.spec.mjs"
    }
}
